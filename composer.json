{
    "name": "laravel-notification-channels/smso",
    "description": "SMSO Notifications Channel for Laravel",
    "homepage": "https://https://github.com/andreisava76/smso-service",
    "require": {
        "php": ">=7.2",
        "illuminate/notifications": "~6.0 || ~7.0 || ~8.0",
        "illuminate/support": "~6.0 || ~7.0 || ~8.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "NotificationChannels\\:channel_namespace\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "NotificationChannels\\:channel_namespace\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "test:coverage": "phpunit --coverage-text --coverage-clover=coverage.clover"
    },
    "config": {
        "sort-packages": true
    }
}
